#!/system/bin/sh
################################################################
# ------------------------------------------------------------ #
# |                        Pure Tweaks                       | #
# ------------------------------------------------------------ #
################################################################
# Increase readahead buffers on eMMC devices
    echo "256" > /sys/block/mmcblk0/bdi/read_ahead_kb
    echo "256" > /sys/block/mmcblk1/bdi/read_ahead_kb

